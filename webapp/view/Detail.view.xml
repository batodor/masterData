<mvc:View controllerName="masterdata.MasterData.controller.Detail" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:footerbar="sap.ushell.ui.footerbar" xmlns:core="sap.ui.core">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" showNavButton="{device>/system/phone}" title="{Title}"
		busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:content>
			<core:Fragment fragmentName="fragment.limitsStandart" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.limitsExpress" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.salesProgram" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.fcaDomestic" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.fcaProduct" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.fcaResource" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.productRecipe" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.strategy" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.growthFactor" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.salesScheme" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.riskType" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.salesDirection" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.incoterms" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.currency" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.uom" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.country" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.rwStation" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.port" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.vesselType" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.materialGroup" type="XML"></core:Fragment>
		</semantic:content>
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
		<semantic:shareInJamAction>
			<semantic:ShareInJamAction id="shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>
		</semantic:shareInJamAction>
		<semantic:saveAsTileAction>
			<footerbar:AddBookmarkButton id="shareTile" title="{detailView>/saveAsTileTitle}"/>
		</semantic:saveAsTileAction>
	</semantic:DetailPage>
</mvc:View>