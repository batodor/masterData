<mvc:View controllerName="masterdata.MasterData.controller.Detail" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:footerbar="sap.ushell.ui.footerbar" xmlns:core="sap.ui.core">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" showNavButton="{device>/system/phone}" title="{Title}"
		busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:content>
			<core:Fragment fragmentName="fragment.limitsStandart" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.limitsExpress" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.salesProgram" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.fcaDomestic" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.fcaProduct" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.fcaResource" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.productRecipeHeader" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.productRecipeItem" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.strategy" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.growthFactor" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.salesScheme" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.riskType" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.salesRegion" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.incoterms" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.currency" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.country" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.rwStation" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.port" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.vesselType" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.materialGroup" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.poq" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.terminal" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.legalEntity" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.branch" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.salesMarket" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.bmqc" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.sbmqc" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.crossBorder" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.productionUnit" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.addressType" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.qualityParameters" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.dqp" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.benchmark" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.qualityParametersUom" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.drq" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.inflationFactor" type="XML"></core:Fragment>
			<core:Fragment fragmentName="fragment.index" type="XML"></core:Fragment>
		</semantic:content>
		
		<semantic:mainAction>
			<semantic:MainAction
				id="tableAdd"
				press="tableAdd"
				text="{i18n>add}" />
		</semantic:mainAction>
		<semantic:positiveAction>
			<semantic:PositiveAction
				id="tableEdit" 
				press="tableEdit"
				text="{i18n>edit}" />
		</semantic:positiveAction>
		<semantic:negativeAction>
			<semantic:NegativeAction
				id="tableDelete" 
				press="tableDelete"
				text="{i18n>delete}" />
		</semantic:negativeAction>
		<semantic:saveAsTileAction>
			<Button 
				id="tableDetails"
				type="Emphasized"
				text="{i18n>items}"
				press="tableDetails"/>
		</semantic:saveAsTileAction>
	</semantic:DetailPage>
</mvc:View>