<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>growthFactor}" class="sapUiContentPadding" id="growthFactorDialog" stretch="{device>/system/phone}">
		<content>
			<Label required="true" text="{i18n>costType}"/>
			<Select selectedKey="{CostType}" custom:key="costType" forceSelection="false" items="{ path: '/dictionaryCostTypeSet', sorter: { path: 'Name' } }" width="100%">
				<core:Item key="{Code}" text="{Code} - {Name}"/>
			</Select>
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>dateFrom}"/>
			<DatePicker dateValue="{DateFrom}" custom:key="dateFrom" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>dateTo}"/>
			<DatePicker dateValue="{DateTo}"/>
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>growthFactor}"/>
			<Input value="{GrowthFactor}" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>uom}"/>
			<Select forceSelection="false" custom:name="UOM" selectedKey="{= ${UOM} === '%' ? '%' : '%' }" width="100%">
				<core:Item key="" text=""/>
				<core:Item key="%" text="%"/>
			</Select>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="growthFactor" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="growthFactor" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="growthFactor" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="growthFactor" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>