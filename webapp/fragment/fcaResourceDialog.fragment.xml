<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>fcaResource}" class="sapUiContentPadding">
		<content>
			<Text text="{i18n>salesProgram}" width="250px"/>
			<Input id="iDialogFCARSalesProgram" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>productionUnit}" width="250px"/>
			<Input id="iDialogFCARProductionUnit" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>material}" width="250px"/>
			<Input id="iDialogFCARMaterial" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateFrom}" width="250px"/>
			<DatePicker id="dpDialogFCARDateFrom" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateTo}" width="250px"/>
			<DatePicker id="dpDialogFCARDateTo" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>minFcaPrice}" width="250px"/>
			<Input id="iDialogFCARMinFcaPrice" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>currency}" width="250px"/>
			<CheckBox id="cbDialogFCARCurrency" width="150px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="fcaResource" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="fcaResource" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="fcaResource" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>