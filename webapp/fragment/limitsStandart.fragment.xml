<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Table mode="SingleSelectLeft" popinLayout="GridLarge" includeItemInSelection="true" id="limitsStandart" visible="false" custom:id="limitsStandart" 
		custom:table="LimitsForStandardRating_MasterDetailN" items="{}" select="onTableSelect" width="auto" 
		updateFinished="onListUpdateFinished" noDataText="{i18n>detailLineItemTableNoDataText}" growing="true" 
		growingScrollToLoad="true" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin">
		<headerToolbar>
			<Toolbar class="white">
				<ToolbarSpacer/>
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column><Text class="black" text="{i18n>legalEntity}"/></Column>
			<Column><Text class="black" text="{i18n>rating}"/></Column>
			<Column minScreenWidth="Tablet" demandPopin="true"><Text class="black" text="{i18n>businessPartnersRole}"/></Column>
			<!--<Column><Text class="black" text="{i18n>dateFrom}"/></Column>-->
			<!--<Column><Text class="black" text="{i18n>dateTo}"/></Column>-->
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{LegalEntityText}"/>
					<Text text="{Rating}"/>
					<Text text="{BusinessPartnerRoleText}"/>
					<!--<Text text="{ path: 'DateFrom', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>-->
					<!--<Text text="{ path: 'DateTo', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>-->
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>