<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>inflationFactor}" class="sapUiContentPadding" id="inflationFactorDialog" stretch="{device>/system/phone}">
		<content>
			<Label required="true" text="{i18n>currency}"/>
			<Input 
				custom:id="currencyPopup"
				custom:set="currency"
				custom:key="currency"
				custom:filters="CurrencyCode,CurrencyName"
				id="currencyPopupinflationFactorValueHelp"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{Currency}" />
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>dateFrom}"/>
			<DatePicker dateValue="{DateFrom}" custom:key="dateFrom" width="100%" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>dateTo}"/>
			<DatePicker dateValue="{DateTo}" width="100%" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>inflationFactor}"/>
			<Input value="{InflationFactor}" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>uom}"/>
			<Select selectedKey="{UOM}" forceSelection="false" width="100%" >
				<core:Item key="" text=""/>
				<core:Item key="%" text="%"/>
			</Select>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="inflationFactor" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="inflationFactor" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="inflationFactor" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="inflationFactor" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>