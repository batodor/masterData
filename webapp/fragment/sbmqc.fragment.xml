<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Table mode="SingleSelectLeft" includeItemInSelection="true" id="sbmqc" visible="false" custom:id="sbmqc" items="{}" select="onTableSelect" width="auto" updateFinished="onListUpdateFinished"
		noDataText="{i18n>detailLineItemTableNoDataText}" growing="true" growingScrollToLoad="true" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin">
		<headerToolbar>
			<Toolbar class="white">
				<SearchField placeholder="{i18n>sbmqcName}" custom:id="sbmqc" custom:key="Name" showSearchButton="true" search="triggerSearch" width="auto"/>
				<ToolbarSpacer/>
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column><Text class="black" text="{i18n>sbmqcCode}"/></Column>
			<Column><Text class="black" text="{i18n>sbmqcName}"/></Column>
			<Column><Text class="black" text="{i18n>bmqcName}"/></Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{Code}"/>
					<Text text="{Name}"/>
					<Text text="{NameBase}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>