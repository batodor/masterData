<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>branch}" class="sapUiContentPadding" id="branchDialog">
		<content>
			<Text text="{i18n>legalEntityName}" width="150px"/>
			<Input value="{LegalEntityName}" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>branchCode}" width="150px"/>
			<Input value="{BranchCode}" custom:key="branchCode" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>branchName}" width="150px"/>
			<Input value="{BranchName}" width="250px" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="branch" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="branch" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="branch" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="branch" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>