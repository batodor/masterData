<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>bmqc}" class="sapUiContentPadding" id="bmqcDialog" stretch="{device>/system/phone}">
		<content>
			<Label required="true" text="{i18n>bmqcCode}"/>
			<Input value="{Code}" maxLength="7" custom:key="bmqcCode"/>
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>bmqcName}"/>
			<TextArea value="{Name}" maxLength="100" rows="5" width="100%"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="bmqc" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="bmqc" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="bmqc" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="bmqc" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>