<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>bmqc}" class="sapUiContentPadding" id="bmqcDialog">
		<content>
			<Text text="{i18n>bmqcCode}" width="300px"/>
			<Input value="{Code}" custom:key="bmqcCode" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>bmqcName}" width="300px"/>
			<Input value="{Name}" width="200px" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="bmqc" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="bmqc" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="bmqc" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>