<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>limitsStandartRating}" class="sapUiContentPadding">
		<content>
			<Text text="{i18n>legalEntity}" width="250px"/>
			<Input id="iDialogLSLegalEntity" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>rating}" width="250px"/>
			<Select id="sDialogLSRating" items="{ path: '/DictionaryRatingSet', sorter: { path: 'Rating' } }" width="150px">
				<core:Item key="{Rating}" text="{Rating}"/>
			</Select>
			<ToolbarSpacer/>
			<Text text="{i18n>businessPartnersRole}" width="250px"/>
			<Select id="sDialogLSBusinessPartnersRole" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>tonnage}" width="250px"/>
			<Input id="iDialogLSTonnage" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>uom}" width="250px"/>
			<Select id="sDialogLSUom" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>paymentTerms}" width="250px"/>
			<Input id="iDialogLSPaymentTerms" width="150px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="limitsStandart" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="limitsStandart" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="limitsStandart" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>