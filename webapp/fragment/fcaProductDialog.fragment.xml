<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>fcaProduct}" class="sapUiContentPadding" id="fcaProductDialog">
		<content>
			<Text text="{i18n>salesProgram}" width="250px"/>
			<Input value="{SalesProgram}" custom:key="salesProgram" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>productCode}" width="250px"/>
			<Input value="{ProductCode}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateFrom}" width="250px"/>
			<DatePicker value="{DateFrom}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateTo}" width="250px"/>
			<DatePicker value="{DateTo}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>minFcaPrice}" width="250px"/>
			<Input value="{MinFcaPrice}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>currency}" width="250px"/>
			<Select selected="{Currency}" width="150px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="fcaProduct" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="fcaProduct" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="fcaProduct" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>