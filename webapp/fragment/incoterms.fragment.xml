<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Table mode="SingleSelectLeft" id="incoterms" visible="false" items="{}" select="onTableSelect" width="auto" updateFinished="onListUpdateFinished"
		noDataText="{i18n>detailLineItemTableNoDataText}" growing="true" growingScrollToLoad="true" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin">
		<headerToolbar>
			<Toolbar class="white">
				<Label id="incotermsCount" text="Items"/>
				<ToolbarSpacer/>
				<SearchField placeholder="{i18n>search}" showSearchButton="true" search="triggerSearch" width="auto"/>
				<!--<Button text="{i18n>add}" custom:id="incoterms" press="tableAdd" type="Accept"/>-->
				<!--<Button id="incotermsEdit" custom:id="incoterms" text="{i18n>edit}" press="tableEdit" enabled="false" type="Emphasized"/>-->
				<!--<Button id="incotermsDelete" custom:id="incoterms" text="{i18n>delete}" press="tableDelete" type="Reject" enabled="false"/>-->
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column><Text class="black" text="{i18n>incoterms}"/></Column>
			<Column><Text class="black" text="{i18n>description}"/></Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{Incoterms}"/>
					<Text text="{Description}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>