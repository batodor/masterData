<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>index}" class="sapUiContentPadding" id="indexDialog" stretch="{device>/system/phone}">
		<content>
			<Label required="true" text="{i18n>index}"/>
			<Input value="{Index}" custom:key="{i18n>index}"/>
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>productType}"/>
			<Select selectedKey="{ProductType}" custom:key="productType" forceSelection="false" items="{ path: '/dictionarySalesDivisionsSet', sorter: { path: 'Name' } }" width="100%">
				<core:Item key="{Code}" text="{Code} - {Name}"/>
			</Select>
			
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>currency}"/>
			<Input 
				custom:id="currencyPopup"
				custom:set="currency"
				custom:key="currency"
				custom:filters="CurrencyCode,CurrencyName"
				id="currencyPopupindexValueHelp"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{Currency}" />
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>incoterms}"/>
			<Select forceSelection="false" selectedKey="{Incoterms}" custom:key="incoterms" items="{ path: '/incotermsSet', sorter: { path: 'Incoterms' } }" width="100%">
				<core:Item key="{Incoterms}" text="{Incoterms} - {Description}"/>
			</Select>
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>dateFrom}"/>
			<DatePicker dateValue="{DateFrom}" custom:key="dateFrom"/>
			<ToolbarSpacer/>
			<Label class="marginTop" required="false" text="{i18n>dateTo}"/>
			<DatePicker dateValue="{DateTo}" custom:editable="true"/>
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>geolocation}"/>
			<Input 
				custom:id="geolocationPopup"
				custom:key="Code" 
                custom:set="dictionaryDeliveryPoints"
				custom:filters="Code,Name,TrainStationID,SeaHarborIndicator,CrossBorderIndicator"
				id="geolocationPopupindexValueHelp"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{Geolocation} - {GeolocationText}" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>baseCV}"/>
			<Input value="{BaseCV}" maxLength="7" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>narGarGad}"/>
			<Select forceSelection="false" selectedKey="{NGG}" width="100%">
				<core:Item key="" text=""/>
				<core:Item key="NAR" text="NAR"/>
				<core:Item key="GAR" text="GAR"/>
				<core:Item key="GAD" text="GAD"/>
			</Select>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="index" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="index" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="index" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="index" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>