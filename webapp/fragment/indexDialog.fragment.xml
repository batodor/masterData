<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>index}" class="sapUiContentPadding" id="indexDialog">
		<content>
			<Label required="true" text="{i18n>index}" width="200px"/>
			<Input value="{Index}" width="200px" custom:key="{i18n>index}" />
			<ToolbarSpacer/>
			<Label required="true" text="{i18n>materialGroup}" width="200px"/>
			<Select selectedKey="{MaterialGroup}" custom:key="materialGroup" forceSelection="false" items="{ path: '/dictionaryMaterialGroupsSet', sorter: { path: 'Name' } }" width="200px">
				<core:Item key="{Code}" text="{Code} - {Name}"/>
			</Select>
			<ToolbarSpacer/>
			<Label required="true" text="{i18n>currency}" width="200px"/>
			<Input 
				custom:id="currencyPopup"
				custom:set="currency"
				custom:key="currency"
				custom:filters="CurrencyCode,CurrencyName"
				id="currencyPopupindexValueHelp"
				placeholder="{i18n>plsChoose} {i18n>currency}"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{Currency}" width="200px" />
			<ToolbarSpacer/>
			<Label required="true" text="{i18n>incoterms}" width="200px"/>
			<Select forceSelection="false" selectedKey="{Incoterms}" custom:key="incoterms" items="{ path: '/incotermsSet', sorter: { path: 'Incoterms' } }" width="200px">
				<core:Item key="{Incoterms}" text="{Incoterms} - {Description}"/>
			</Select>
			<ToolbarSpacer/>
			<Label required="true" text="{i18n>dateFrom}" width="200px"/>
			<DatePicker dateValue="{DateFrom}" custom:key="dateFrom" width="206px"/>
			<ToolbarSpacer/>
			<Label required="true" text="{i18n>dateTo}" width="200px"/>
			<DatePicker dateValue="{DateTo}" custom:key="dateTo" width="206px"/>
			<ToolbarSpacer/>
			<Label text="{i18n>geolocation}" width="200px"/>
			<Input 
				custom:id="geolocationPopup"
				custom:set="dictionaryTransportConnectionPoints"
				custom:filters="Code,Name,TrainStationID,SeaHarborIndicator,CrossBorderIndicator"
				id="geolocationPopupindexValueHelp"
				placeholder="{i18n>plsChoose} {i18n>geolocation}"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{Geolocation}" width="200px" />
			<ToolbarSpacer/>
			<Label text="{i18n>baseCV}" width="200px"/>
			<Input value="{BaseCV}" width="200px" />
			<ToolbarSpacer/>
			<Label text="{i18n>baseQP}" width="200px"/>
			<Select forceSelection="false" selectedKey="{BaseQP}" items="{ path: '/bmqcSet', sorter: { path: 'Code' } }" width="200px">
				<core:Item key="{Code}" text="{Code} - {Name}"/>
			</Select>
			<ToolbarSpacer/>
			<Label text="{i18n>narGarGad}" width="200px"/>
			<Select forceSelection="false" selectedKey="{NGG}" width="200px">
				<core:Item key="" text=""/>
				<core:Item key="NAR" text="NAR"/>
				<core:Item key="GAR" text="GAR"/>
				<core:Item key="GAD" text="GAD"/>
			</Select>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="index" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="index" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="index" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="index" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>