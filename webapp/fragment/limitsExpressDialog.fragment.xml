<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>limitsExpressRating}" class="sapUiContentPadding">
		<content>
			<Text text="{i18n>legalEntity}" width="250px"/>
			<Input id="iDialogLELegalEntity" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>contryRating}" width="250px"/>
			<Select id="sDialogLECountryRating" items="{ path: '/DictionaryRatingSet', sorter: { path: 'Rating' } }" width="150px">
				<core:Item key="{Rating}" text="{Rating}"/>
			</Select>
			<ToolbarSpacer/>
			<Text text="{i18n>financeCommercialRating}" width="250px"/>
			<Select id="sDialogLEFinanceCommercialRating" items="{ path: '/DictionaryRatingSet', sorter: { path: 'Rating' } }" width="150px">
				<core:Item key="{Rating}" text="{Rating}"/>
			</Select>
			<ToolbarSpacer/>
			<Text text="{i18n>businessPartnersRole}" width="250px"/>
			<Select id="sDialogLEBusinessPartnersRole" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>paymentTerms}" width="250px"/>
			<Input id="iDialogLEPaymentTerms" width="150px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="limitsExpress" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="limitsExpress" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="limitsExpress" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>