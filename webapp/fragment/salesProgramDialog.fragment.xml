<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>salesProgram}" class="sapUiContentPadding">
		<content>
			<Text text="{i18n>salesProgram}" width="150px"/>
			<Input id="iDialogSPSalesProgram" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>description}" width="150px"/>
			<TextArea id="taDialogSPDescription" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>materialGroup}" width="150px"/>
			<Input id="iDialogSPMaterialGroup" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>incoterms}" width="150px"/>
			<Input id="iDialogSPIncoterms" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>cvNar}" width="150px"/>
			<Input id="iDialogSPCvNar" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>uom}" width="150px"/>
			<Input id="iDialogSPUom" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>narGarGad}" width="150px"/>
			<Input id="iDialogSPNarGarGad" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>actual}" width="150px"/>
			<CheckBox id="cbDialogSPActual" width="250px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="salesProgram" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="salesProgram" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="salesProgram" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>