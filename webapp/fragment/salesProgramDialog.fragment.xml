<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>salesProgram}" class="sapUiContentPadding" id="salesProgramDialog">
		<content>
			<Text text="{i18n>salesProgram}" width="150px"/>
			<Input value="{SalesProgram}" custom:key="thisInputIsKey" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>description}" width="150px"/>
			<TextArea value="{Description}" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>incoterms}" width="150px"/>
			<Input value="{Incoterms}" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>cvNar}" width="150px"/>
			<Input value="{CV}" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>uom}" width="150px"/>
			<Input value="{Uom}" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>narGarGad}" width="150px"/>
			<Input value="{NarGarGad}" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>active}" width="150px"/>
			<CheckBox selected="{Active}" width="250px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="salesProgram" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="salesProgram" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="salesProgram" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>