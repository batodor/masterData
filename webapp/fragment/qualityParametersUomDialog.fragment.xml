<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>qualityParametersUom}" class="sapUiContentPadding" id="qualityParametersUomDialog" stretch="{device>/system/phone}">
		<content>
			<Label required="true" text="{i18n>qpCode}"/>
			<Input 
				custom:id="qualityParametersPopup"
				custom:key="qpCode"
				custom:filters="QPCode,QPName"
				custom:set="qualityParameters"
				id="qualityParametersPopupqualityParametersUomValueHelp"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{QPCode}" />
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>uom}"/>
			<Input 
				custom:id="uom"
				custom:key="uom"
				custom:filters="UOMCode,UOMName"
				id="uomqualityParametersUomValueHelp"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{UOM}" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>minValue}"/>
			<Input
				value="{MinValue}"
				type="Number"
				maxLength="19"
				change="checkDecimal"
				custom:type="Number"
				placeholder="0"/>
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>maxValue}"/>
			<Input
				value="{MaxValue}"
				type="Number"
				maxLength="19"
				change="checkDecimal"
				custom:type="Number"
				placeholder="0" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>normalPrecision}"/>
			<Input value="{NormalPrecision}" maxLength="1" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="qualityParametersUom" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="qualityParametersUom" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="qualityParametersUom" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="qualityParametersUom" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>