<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>country}" class="sapUiContentPadding" id="countryDialog">
		<content>
			<Text text="{i18n>countryCode}" width="150px"/>
			<Input custom:key="thisInputIsKey" value="{Code}" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>countryName}" width="150px"/>
			<Input custom:key="thisInputIsKey" value="{Name}" width="250px" />
			<ToolbarSpacer/>
			<Text text="{i18n>blacklisted}" width="150px"/>
			<CheckBox selected="{Sanction}" width="250px" />
			<ToolbarSpacer/>
			<Text text="{i18n>countryRating}" width="150px"/>
			<Select selectedKey="{Rating}" forceSelection="false" items="{ path: '/dictionaryRatingSet', sorter: { path: 'Rating' } }" width="250px">
				<core:Item key="{Rating}" text="{Rating}"/>
			</Select>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="country" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="country" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="country" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>