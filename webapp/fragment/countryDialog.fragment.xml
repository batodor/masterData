<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>country}" stretch="{device>/system/phone}" class="sapUiContentPadding" id="countryDialog">
		<content>
			<Label required="true" text="{i18n>countryCode}" />
			<Input custom:key="countryCode" value="{Code}"/>
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>countryName}"/>
			<Input custom:key="countryName" value="{Name}"/>
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>sanction}"/>
			<ToolbarSpacer/>
			<CheckBox selected="{Sanction}"/>
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>countryRating}"/>
			<Select selectedKey="{Rating}" forceSelection="false" items="{ path: '/dictionaryRatingSet', sorter: { path: 'Rating' } }" width="100%">
				<core:Item key="{Rating}" text="{Rating}"/>
			</Select>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="country" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="country" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="country" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="country" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>