<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>dqp}" class="sapUiContentPadding" id="dqpDialog">
		<content>
			<Text text="{i18n>typeOfProduct}" width="150px"/>
			<Input value="{TypeOfProduct}" custom:key="typeOfProduct" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>salesProgram}" width="150px"/>
			<Input value="{MaterialName}" custom:key="salesProgram" width="200px" />
			<ToolbarSpacer/>
			<Text text="{i18n>salesRegion}" width="150px"/>
			<Input value="{SalesRegion}" custom:key="salesRegion" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>qp}" width="150px"/>
			<Input value="{QP}" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>baseQP}" width="150px"/>
			<Input value="{BaseQP}" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>uom}" width="150px"/>
			<Input value="{UOM}" width="200px"/>
			<ToolbarSpacer/>
			
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="dqp" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="dqp" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="dqp" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>