<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>incoterms}" class="sapUiContentPadding" id="incotermsDialog">
		<content>
			<Label required="true" text="{i18n>incoterms}" width="150px"/>
			<Input custom:key="incoterms" value="{Incoterms}" width="250px"/>
			<ToolbarSpacer/>
			<Label text="{i18n>description}" width="150px"/>
			<TextArea value="{Description}" width="250px" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="incoterms" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="incoterms" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>save}" custom:id="incoterms" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>