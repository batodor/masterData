<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Table mode="SingleSelectLeft" id="fcaDomestic" visible="false" custom:id="fcaDomestic" items="{}" select="onTableSelect" width="auto" updateFinished="onListUpdateFinished"
		noDataText="{i18n>detailLineItemTableNoDataText}" growing="true" growingScrollToLoad="true" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin">
		<headerToolbar>
			<Toolbar class="white">
				<Label id="fcaDomesticCount" text="Items"/>
				<ToolbarSpacer/>
				<SearchField placeholder="{i18n>search}" showSearchButton="true" search="triggerSearch" width="auto"/>
				<Button text="{i18n>add}" custom:id="fcaDomestic" press="tableAdd" type="Accept"/>
				<Button id="fcaDomesticEdit" custom:id="fcaDomestic" text="{i18n>edit}" press="tableEdit" enabled="false" type="Emphasized"/>
				<Button id="fcaDomesticDelete" custom:id="fcaDomestic" text="{i18n>delete}" press="tableDelete" type="Reject" enabled="false"/>
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column><Text class="black" text="{i18n>salesRegion}"/></Column>
			<Column><Text class="black" text="{i18n>productionUnit}"/></Column>
			<Column><Text class="black" text="{i18n>material}"/></Column>
			<Column><Text class="black" text="{i18n>distributionInBulk}"/></Column>
			<Column><Text class="black" text="{i18n>dateFrom}"/></Column>
			<Column><Text class="black" text="{i18n>dateTo}"/></Column>
			<Column><Text class="black" text="{i18n>fcaPriceRub}"/></Column>
			<Column><Text class="black" text="{i18n>fcaPriceUsd}"/></Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{SalesRegion}"/>
					<Text text="{ProductionUnit}"/>
					<Text text="{Material}"/>
					<Text text="{DistributionInBulk}"/>
					<Text text="{DateFrom}"/>
					<Text text="{DateTo}"/>
					<Text text="{FcaPriceRub}"/>
					<Text text="{FcaPriceUsd}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>