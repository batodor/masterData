<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>fcaDomestic}" class="sapUiContentPadding" id="fcaDomesticDialog">
		<content>
			<Text text="{i18n>salesRegion}" width="250px"/>
			<Input value="{SalesRegion}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>productionUnit}" width="250px"/>
			<Input value="{ProductionUnit}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>material}" width="250px"/>
			<Input value="{Material}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>distributionInBulk}" width="250px"/>
			<Input value="{DistributionInBulk}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateFrom}" width="250px"/>
			<DatePicker value="{DateFrom}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateTo}" width="250px"/>
			<DatePicker value="{DateTo}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>fcaPriceRub}" width="250px"/>
			<Input value="{FcaPriceRub}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>fcaPriceUsd}" width="250px"/>
			<Input value="{FcaPriceUsd}" width="150px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="fcaDomestic" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="fcaDomestic" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="fcaDomestic" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>