<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>qualityParameters}" class="sapUiContentPadding" id="qualityParametersDialog">
		<content>
			<Text text="{i18n>qpKey}" width="300px"/>
			<Input value="{Key}" custom:key="qpKey" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>qpCode}" width="300px"/>
			<Input value="{Code}" width="200px" />
			<ToolbarSpacer/>
			<Text text="{i18n>qpName}" width="300px"/>
			<Input value="{Name}" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>setOfBaseQP}" width="300px"/>
			<Input value="{SetOfBaseQP}" width="200px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>finenessQP}" width="300px"/>
			<Input value="{FinenessQP}" width="200px"/>
			<ToolbarSpacer/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="qualityParameters" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="qualityParameters" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="qualityParameters" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>