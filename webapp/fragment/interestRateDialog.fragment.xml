<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>interestRate}" class="sapUiContentPadding" id="interestRateDialog" stretch="{device>/system/phone}">
		<content>
			<Label required="true" text="{i18n>currency}" />
			<Input 
				custom:id="currencyPopup"
				custom:set="currency"
				custom:key="currency"
				custom:filters="CurrencyCode,CurrencyName"
				id="currencyPopupinterestRateValueHelp"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{Currency}"  />
			<ToolbarSpacer/>
			<Label class="marginTop" required="true" text="{i18n>dateFrom}" />
			<DatePicker custom:key="dateFrom" dateValue="{DateFrom}" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>country}" required="false" />
			<Input 
				custom:id="countryPopup"
				custom:set="country"
				custom:key="country"
				custom:noCheck="true"
				custom:filters="Code,Name"
				id="countryPopupinterestRateValueHelp"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{Country}" />
			<ToolbarSpacer/>
			<Label class="marginTop" required="false" text="{i18n>dateTo}" />
			<DatePicker custom:editable="true" dateValue="{DateTo}" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>interestRate}" />
			<Input value="{InterestRate}" type="Number" change="checkValue" custom:max="99999" custom:type="Number" placeholder="0"  />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>uom}" />
			<Select forceSelection="false" custom:name="UOM" selectedKey="{= ${UOM} === '%' ? '%' : '%' }" width="100%">
				<core:Item key="" text=""/>
				<core:Item key="%" text="%"/>
			</Select>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="interestRate" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="interestRate" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="interestRate" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="interestRate" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>