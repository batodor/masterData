<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>strategy}" class="sapUiContentPadding" id="strategyDialog">
		<content>
			<Label required="true" text="{i18n>strategyCode}" width="150px"/>
			<Input value="{StrategyCode}" custom:key="strategyCode" width="250px"/>
			<ToolbarSpacer/>
			<Label text="{i18n>description}" width="150px"/>
			<TextArea value="{Description}" width="250px" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="strategy" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="strategy" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>save}" custom:id="strategy" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>