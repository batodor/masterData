<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>strategy}" id="strategyDialog" class="sapUiContentPadding">
		<content>
			<Text text="{i18n>strategyCode}" width="150px"/>
			<Input id="iDialogSStrategyCode" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>description}" width="150px"/>
			<TextArea id="taDialogSDescription" width="250px" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="strategy" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="strategy" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="strategy" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>