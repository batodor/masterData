<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>productionUnit}" class="sapUiContentPadding" id="productionUnitDialog">
		<content>
			<Text text="{i18n>productionUnitCode}" width="250px"/>
			<Input value="{Code}" custom:key="thisInputIsKey" width="250px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>productionUnitName}" width="250px"/>
			<Input value="{Name}" width="250px" />
			<ToolbarSpacer/>
			<Text text="{i18n>internalExternal}" width="250px"/>
			<Input value="{internalExternal}" custom:key="thisInputIsKey" width="250px" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="productionUnit" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="productionUnit" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="productionUnit" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>