<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Table mode="SingleSelectLeft" includeItemInSelection="true" id="dqp" visible="false" custom:id="dqp" items="{}" select="onTableSelect" width="auto" updateFinished="onListUpdateFinished"
		noDataText="{i18n>detailLineItemTableNoDataText}" growing="true" growingScrollToLoad="true" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin">
		<headerToolbar>
			<Toolbar class="white">
				<SearchField placeholder="{i18n>materialName}" custom:id="dqp" custom:key="MaterialName" showSearchButton="true" search="triggerSearch" width="auto"/>
				<ToolbarSpacer/>
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column><Text class="black" text="{i18n>productionUnitName}"/></Column>
			<Column><Text class="black" text="{i18n>materialName}"/></Column>
			<Column><Text class="black" text="{i18n>qpKey}"/></Column>
			<Column><Text class="black" text="{i18n>qpBase}"/></Column>
			<Column><Text class="black" text="{i18n>uom}"/></Column>
			<Column><Text class="black" text="{i18n>defaultValue}"/></Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{ProductionUnitName}"/>
					<Text text="{MaterialName}"/>
					<Text text="{QPKey}"/>
					<Text text="{QPBase}"/>
					<Text text="{Uom}"/>
					<Text text="{DefaultValue}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>