<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>benchmark}" class="sapUiContentPadding" id="benchmarkDialog">
		<content>
			<Text text="{i18n>salesProgram}" width="250px"/>
			<Input value="{CodeSalesProgram}" custom:key="salesProgram" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateFrom}" width="250px"/>
			<DatePicker value="{DateFrom}" custom:key="dateFrom" width="155px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>dateTo}" width="250px"/>
			<DatePicker value="{DateTo}" custom:key="dateTo" width="155px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>product}" width="250px"/>
			<Input value="{Product}" custom:key="product" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>salesMarket}" width="250px"/>
			<Input value="{SalesMarket}" custom:key="salesMarket" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>incoterms}" width="250px"/>
			<Select selectedKey="{Incoterms}" forceSelection="false" 
				items="{ path: '/incotermsSet', sorter: { path: 'Incoterms' } }" width="150px">
				<core:Item key="{Incoterms}" text="{Incoterms} - {Description}"/>
			</Select>
			<ToolbarSpacer/>
			<Text text="{i18n>transshipmentPoint}" width="250px"/>
			<Input 
				custom:id="portPopup"
				custom:set="port"
				custom:filters="Country,Code,Name,Station"
				id="portPopupbenchmarkValueHelp"
				placeholder="{i18n>plsEnter} {i18n>port}"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{TransshipmentPoint}" width="150px" />
			<ToolbarSpacer/>
			<Text text="{i18n>cvNar}" width="250px"/>
			<Input value="{CVNAR}" width="150px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>uom}" width="250px"/>
			<Input 
				custom:id="uom"
				custom:filters="UOMCode,UOMName"
				id="uombenchmarkValueHelp"
				placeholder="{i18n>plsEnter} {i18n>uom}"
				showValueHelp="true"
				valueHelpRequest="handleValueHelp"
				value="{UOM}" width="150px" />
			<ToolbarSpacer/>
			<Text text="{i18n>formulaBenchmark}" width="250px"/>
			<Input value="{Formula}" width="150px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="benchmark" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="benchmark" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="benchmark" visible="false" type="Accept" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>