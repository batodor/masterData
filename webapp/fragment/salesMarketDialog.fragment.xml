<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>salesMarket}" stretch="{device>/system/phone}" class="sapUiContentPadding" id="salesMarketDialog">
		<content>
			<Label required="true" text="{i18n>salesMarketCode}" />
			<Input value="{Code}" id="salesMarketInputCode" custom:key="salesMarketCode" />
			<ToolbarSpacer/>
			<Label class="marginTop" text="{i18n>salesMarketName}" />
			<Input value="{Name}" />
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="salesMarket" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="salesMarket" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>edit}" custom:id="salesMarket" visible="false" type="Default" press="dialogEdit"/>
			<Button text="{i18n>save}" custom:id="salesMarket" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>