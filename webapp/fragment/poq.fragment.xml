<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Table mode="SingleSelectLeft" id="poq" visible="false" custom:id="poq" items="{}" select="onTableSelect" width="auto" updateFinished="onListUpdateFinished"
		noDataText="{i18n>detailLineItemTableNoDataText}" growing="true" growingScrollToLoad="true" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin">
		<headerToolbar>
			<Toolbar class="white">
				<Label id="poqCount" text="Items"/>
				<ToolbarSpacer/>
				<SearchField placeholder="{i18n>search}" showSearchButton="true" search="triggerSearch" width="auto"/>
				<Button text="{i18n>add}" custom:id="poq" press="tableAdd" type="Accept"/>
				<Button id="poqEdit" custom:id="poq" text="{i18n>edit}" press="tableEdit" enabled="false" type="Emphasized"/>
				<Button id="poqDelete" custom:id="poq" text="{i18n>delete}" press="tableDelete" type="Reject" enabled="false"/>
			</Toolbar>
		</headerToolbar>
		<columns>
			<Column><Text class="black" text="{i18n>uom}"/></Column>
			<Column><Text class="black" text="{i18n>qpCode}"/></Column>
			<Column><Text class="black" text="{i18n>qpName}"/></Column>
			<Column><Text class="black" text="{i18n>setOfBaseForMeasuringQPName}"/></Column>
			<Column><Text class="black" text="{i18n>minValue}"/></Column>
			<Column><Text class="black" text="{i18n>maxValue}"/></Column>
			<Column><Text class="black" text="{i18n>normalPrecision}"/></Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{UOM}"/>
					<Text text="{QPCode}"/>
					<Text text="{QPName}"/>
					<Text text="{SetOfBaseForMeasuringQPName}"/>
					<Text text="{MinValue}"/>
					<Text text="{MaxValue}"/>
					<Text text="{NormalPrecision}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>